{
    "manifest_version": 2,
    "name": "Webflix",
    "description": "Watch movies right from your extensions!",
    "version": "0.1.0",
    "icons": {
        "16": "assets/16.png",
        "32": "assets/32.png",
        "48": "assets/48.png"
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs"
    ]
}
